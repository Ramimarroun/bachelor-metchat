{
  "name": "met-chat",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "axios": "^1.3.3",
    "buffer": "^6.0.3",
    "core-js": "^2.5.7",
    "emoji-picker-react": "^4.4.7",
    "if-env": "1.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.6.1",
    "styled-components": "^5.3.6",
    "web-vitals": "^2.1.0",
    "yarn": "^1.22.19"
  },
  "main": "client/src/App.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd server && npm run back",
    "start:dev": "concurrently \"npm run server\" \"npm run client\" -k",
    "client": "cd client && npm run start",
    "server": "cd server && npm run back",
    "install": "cd client && npm install && cd ../server && npm install",
    "build": "cd client && npm run build"
  },
  "author": "Rami Maroun, Narges Rezaei, Aina Stustad-Meren, and Fatima Yasefi",
  "license": "OsloMet",
  "devDependencies": {
    "concurrently": "8.0.1"
  }
}
